{
  "name": "building-viewer-web",
  "version": "1.0.0",
  "description": "Web app to view a 3D building with metrics",
  "private": true,
  "scripts": {
    "build": "rimraf dist && webpack --bail --progress --profile",
    "server": "webpack-dev-server --history-api-fallback --inline --progress --host 0.0.0.0",
    "test": "karma start",
    "test-watch": "karma start --auto-watch --no-single-run",
    "start": "npm run server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/enric-sinh/building-viewer.git"
  },
  "author": "Enric Sala",
  "homepage": "https://github.com/enric-sinh/building-viewer",
  "dependencies": {
    "angular": "^1.5.7",
    "moment": "^2.17.1",
    "rainbowvis.js": "^1.0.1",
    "suncalc": "^1.8.0",
    "three": "^0.78.0",
    "tween.js": "^16.3.5"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.6",
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.19.0",
    "isparta-instrumenter-loader": "^1.0.0",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
